{
  "agent": {
    "run_as_user": "root",
    "debug": true,
    "credentials": {
      "role_arn": "{integ-test-role-arn}"
    }
  },
  "logs": {
    "endpoint_override": "https://logs-perf.us-east-1.amazonaws.com",
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/tmp/cwagent_log_test.log*",
            "log_group_name": "{instance_id}",
            "log_stream_name": "{instance_id}",
            "timezone": "UTC"
          }
        ]
      }
    }
  }
}
